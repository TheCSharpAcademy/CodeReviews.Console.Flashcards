> create deck of falshcard
> 1 talbe deck 1 table flash-card 1 table study sensions
> stack shold have unique name
> table study area - date and score and Deck id as a foreign key


create table deck(
Id INT PRIMARY KEY NOT NULL IDENTITY,
Name_ VARCHAR (100) NOT NULL UNIQUE,
Description VARCHAR (255));

create table cards(
Id INT PRIMARY KEY NOT NULL IDENTITY,
Front VARCHAR(100) NOT NULL,
Back VARCHAR(100) NOT NULL,
DeckFk INT not null,
foreign key (DeckFk) references deck(Id) on delete cascade);


create table studySession(
Id INT PRIMARY KEY NOT NULL IDENTITY,
User_ VARCHAR(100),
Init_ dateTime,
Fin dateTime,
Score INT,
StackFk INT NOT NULL,
foreign key (StackFk) references deck(Id) on delete cascade);
